{
  "name": "homelab-service-manager-api",
  "version": "1.0.0",
  "description": "A REST API for managing homelab services with security monitoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frasse007/homelab-manager.git"
  },
  "keywords": [
    "homelab",
    "api",
    "rest",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "Rasmus Westerlund",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Frasse007/homelab-manager/issues"
  },
  "homepage": "https://github.com/Frasse007/homelab-manager#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "path": "^0.12.7",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "supertest": "^7.2.2"
  }
}
